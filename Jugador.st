'From Pharo12.0.0SNAPSHOT of 2 August 2024 [Build information: Pharo-12.0.0+SNAPSHOT.build.1526.sha.404d3af7531f656b5c294762c79011c10bea1ef3 (64 Bit)] on 8 November 2024 at 9:28:24.383 pm'!Object subclass: #Jugador	instanceVariableNames: 'vidas miniJuego territorio contrincantes nombre'	classVariableNames: ''	package: 'AAA JuegoTPI'!!Jugador methodsFor: 'initialization' stamp: '11/8/2024 19:03'!initialize 	"Inicializar jugador nuevo"vidas := 1.territorio := OrderedCollection new.contrincantes := OrderedCollection new.! !!Jugador methodsFor: 'as yet unclassified' stamp: '11/8/2024 19:03'!territorioGanado: territRival	"Asignar el territorio de Rival al vencedor del combate"| bandera |territRival do: [ :i|		bandera := true.	territorio do: [ :t|		(t = i) ifTrue: [ 			bandera := false 			].		 ].		(bandera) ifTrue: [ 		territorio add: i 		].		].! !!Jugador methodsFor: 'as yet unclassified' stamp: '11/8/2024 19:04'!territorioPerdido	"Eliminar territorio de un jugador"territorio := nil.! !!Jugador methodsFor: 'as yet unclassified' stamp: '11/8/2024 19:06'!ganarVida	"Ganar vida despues del desaio"vidas := vidas + 1.! !!Jugador methodsFor: 'as yet unclassified' stamp: '11/8/2024 19:06'!perderVida	"Perder vida despues del desafio"vidas := vidas - 1.! !!Jugador methodsFor: 'as yet unclassified' stamp: '11/8/2024 21:14'!territorioInicial: valoresPos	"Asigna primer casillero al jugador"territorio add: valoresPos.! !!Jugador methodsFor: 'as yet unclassified' stamp: '11/8/2024 19:07'!contrincantes: posicion dim: dimension	"Genero la primera lista de contrincantes"| filaC colC nuevoCasi |filaC := posicion fila.colC := posicion columna."Izquierda"(filaC - 1) > (0) ifTrue: [ 		nuevoCasi := Casillero new.	nuevoCasi fila: (filaC - 1).	nuevoCasi columna: colC.	contrincantes add: nuevoCasi		]."Derecha"(filaC + 1) <= (dimension) ifTrue: [ 		nuevoCasi := Casillero new.	nuevoCasi fila: (filaC + 1).	nuevoCasi columna: colC.	contrincantes add: nuevoCasi		]."Abajo"(colC - 1) > (0) ifTrue: [ 		nuevoCasi := Casillero new.	nuevoCasi fila: filaC.	nuevoCasi columna: (colC - 1).	contrincantes add: nuevoCasi		]."Arriba"(colC + 1) <= (dimension) ifTrue: [ 		nuevoCasi := Casillero new.	nuevoCasi fila: filaC.	nuevoCasi columna: (colC + 1).	contrincantes add: nuevoCasi		].! !!Jugador methodsFor: 'as yet unclassified' stamp: '11/8/2024 19:07'!perderContrincantes	"Perder contrincantes luego del combate"contrincantes := nil.! !!Jugador methodsFor: 'accessing' stamp: '11/8/2024 19:07'!territorio	"Getter de territorio"^territorio! !!Jugador methodsFor: 'accessing' stamp: '11/8/2024 19:07'!vidas	"Getter de vidas"^vidas! !!Jugador methodsFor: 'accessing' stamp: '11/8/2024 19:07'!contrincantes	"Getter de Contrincantes"^contrincantes.! !!Jugador methodsFor: 'accessing' stamp: '11/8/2024 19:08'!miniJuego: instanciaJuego	"Asignacion de nuevo juego"miniJuego:= instanciaJuego.! !!Jugador methodsFor: 'accessing' stamp: '11/8/2024 19:48'!nombre	"Getter de nombre"	^nombre! !!Jugador methodsFor: 'accessing' stamp: '11/8/2024 19:48'!nombre: unNombre	"Setter de nombre"nombre := unNombre! !